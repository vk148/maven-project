pipeline {
    agent any
    
stages{
        stage('Build'){
            steps {
			    bat 'set PATH=%PATH%;C:\Softwares\apache-maven-3.3.9\bin'
                bat 'mvn clean package'
            }
            post {
                success {
                    echo 'Now Archiving...'
                    archiveArtifacts artifacts: '**/target/*.war'
                }
            }
        }
    }
}