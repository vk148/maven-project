pipeline {
    agent any
tools {
    maven 'localmaven'
	java 'JDK1.8'
  }    
stages{
        stage('Build'){
            steps {
				bat 'mvn -version'
			    bat 'mvn clean package'
            }
            post {
                success {
                    echo 'Now Archiving...'
                    archiveArtifacts artifacts: '**/target/*.war'
                }
            }
        }
    }
}