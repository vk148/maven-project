pipeline {
    agent any
tools {
    maven 'localmaven'
  }    
stages{
        stage('Build'){
            steps {
			    bat 'C:\\Softwares\\apache-maven-3.3.9\\bin\\mvn.bat clean package'
            }
            post {
                success {
                    echo 'Now Archiving...'
                    archiveArtifacts artifacts: '**/target/*.war'
                }
            }
        }
    }
}